{% set aggs = aggs|default([]) -%}

{
  "title": "qpi",
  "type": "line",
  "listeners": {},
  "params": {
    "addLegend": true,
    "shareYAxis": true,
    "addTooltip": true,
    "smoothLines": false,
    "scale": "linear",
    "interpolate": "linear",
    "times": [],
    "addTimeMarker": false,
    "defaultYExtents": false,
    "setYExtents": false,
    "yAxis": {},
    "mode": "stacked"
  },
  "aggs": [
    {% for agg in aggs %}
    {
      "id": {{agg.id }},
      "type": "avg",
      "schema": "metric",
      "params": {
        "field": "{{agg.field}}"
      }
    },
    {% endfor %}
    {
      "id": {{ aggs|length + 1 }},
      "type": "date_histogram",
      "schema": "segment",
      "params": {
        "field": "start_date",
        "interval": "auto",
        "customInterval": "2h",
        "min_doc_count": 1,
        "extended_bounds": {}
      }
    }
  ]
}
